c:{"type":"canvas","size":[100,100],"pos":[0,0],"locked":1,"animated":1,"volatile":1,"script":"wigglyPlayer.1p","border":1,"scale":1}
fr:{"type":"field","size":[100,20],"pos":[0,-77],"locked":1,"show":"none"}
or:{"type":"field","size":[100,20],"pos":[0,-49],"locked":1,"show":"none","style":"plain"}
fl:{"type":"button","size":[60,20],"pos":[0,148],"locked":1,"show":"none","style":"check"}
em:{"type":"field","size":[100,20],"pos":[109,-49],"locked":1,"show":"none","style":"plain"}
cbo:{"type":"button","size":[100,20],"pos":[109,0],"locked":1,"show":"none","style":"check","value":1}
cst:{"type":"button","size":[100,20],"pos":[109,31],"locked":1,"show":"none","style":"check","value":1}
cd:{"type":"button","size":[100,20],"pos":[109,61],"locked":1,"show":"none","style":"check","value":0}

{script:wigglyPlayer.0p}
on frames do fr.images end
on order  do or.data end

on set_value x do
 if x.frames fr.images:x.frames..copy[] end
 or.data: (range count x.frames) unless x.order
end
on get_value do
 r.frames:frames[]
 r.order :order[]
 r
end

on view do
 f:frames[]
 o:order[]
 c.border:cbo.value
 c.show:card.show
 c.draggable:cd.value
 c.clear[]
 if count f
  i:f[o[(count o)%sys.frame/5]]
  i:if fl.value i.copy[].transform["horiz"] else i end
  if get_stretch[]
   c.paste[i 0,0,card.size]
  else
   c.paste[i .5*c.size-i.size]
  end
 end
end

on get_border do cbo.value end
on set_border x do c.border:x cbo.value:c.border end
on get_stretch do cst.value end
on set_stretch x do cst.value:x end
on get_frames do fr.value end
on set_frames x do fr.value:x end
on get_order_text do "," fuse order[] end
on set_order_text x do or.data: 0+"," split x end
on get_animate do view end
on get_flip do fl.value end
on set_flip x do fl.value:x end
on get_emotes do em.data end
on set_emotes x do em.data: x end
on set_emote x do or.data:() unless get_emotes[][x] end
on get_draggable do cd.value end
on set_draggable x do cd.value:x end

{end}

{script:wigglyPlayer.1p}
on click pos do
 card.event["click" pos]
end
on drag pos do
 if get_draggable[]
  card.pos:pos+(pointer.pos-pointer.start)
  me.pos:0,0
  card.event["drag" pos]
 end
end
on release pos do
 if get_draggable[]
  card.event["release" pos]
 end
end
{end}

