{contraption:wigglyCanvas}
size:[100,100]
resizable:1
margin:[1,1,1,1]
description:"A simple wiggly drawing surface compatible with wigglyPlayer."
version:1.1
script:"wigglyCanvas.0p"
template:"on stroke_start do\n \nend\n\non stroke_end do\n \nend"
attributes:{"name":["border","temp"],"label":["Border","Template Background"],"type":["bool","bool"]}
{widgets}
c:{"type":"canvas","size":[100,100],"pos":[0,0],"animated":1,"volatile":1,"border":1,"brush":1,"scale":1}
fr:{"type":"field","size":[100,20],"pos":[0,-77],"locked":1,"show":"none","value":{"text":["","i","i","i"],"font":["","","",""],"arg":["","%%IMG2AGQAZAD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wA3","%%IMG2AGQAZAD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wA3","%%IMG2AGQAZAD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wD/AP8A/wA3"]}}
sfx:{"type":"grid","size":[100,31],"pos":[0,-49],"locked":1,"show":"none","format":"ss","value":{"name":["pen1","pen2"],"data":["%%","%%"]}}
tb:{"type":"button","size":[60,20],"pos":[0,148],"locked":1,"show":"none","style":"check"}
cbo:{"type":"button","size":[100,20],"pos":[154,0],"locked":1,"show":"none","style":"check","value":1}
cp:{"type":"field","size":[100,20],"pos":[154,32],"locked":1,"show":"none","style":"plain","value":"1"}
cbr:{"type":"field","size":[100,20],"pos":[154,63],"locked":1,"show":"none","style":"plain","value":"3"}

{script:wigglyCanvas.0p}
on frames do fr.images end
on get_value do
 r.frames:frames[]
 r.order:range count r.frames
 r
end
on set_value x do
 if x.frames fr.images: x.frames end
end

on drag x do
 if !card.locked
  fr:frames[]
  c.pattern:get_pattern[]
  c.brush:get_brush[]
  o:if pointer.down x else pointer.prev-card.offset end
  each f in fr
   c.paste[f]
   c.line[o random[-1,0,1 2]+x]
   f.paste[c.copy[]]
   if tb.value
    f.paste[deck.card.image.copy[card.pos card.size] 0,0 1]
   end
  end
  if !app.playing
   play[sound[random[sfx.value.data]]]
  end
 end
end
on click x do
 if !card.locked
  card.event["stroke_start"]
  drag[x]
 end
end
on release x do
 if !card.locked
  card.event["stroke_end"]
 end
end

on view do
 f:frames[]
 c.border:cbo.value
 c.show:card.show
 c.clear[]
 i:f[(count f)%sys.frame/5]
 i.size:c.size
 c.paste[i]
end

on get_animate do view end
on get_border do cbo.value end
on set_border x do c.border:x cbo.value:c.border end
on get_brush do 0+cbr.text end
on set_brush x do c.brush:x cbr.text:c.brush end
on get_pattern do 0+cp.text end
on set_pattern x do c.pattern:x cp.text:c.pattern end
on get_temp do tb.value end
on set_temp x do tb.value:x end
on get_clear do
 on clear do
  fr.value:raze rtext.cat @ (3 take image[])..copy[]
 end
end
{end}

